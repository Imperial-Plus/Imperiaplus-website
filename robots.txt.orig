            speed: 1000,
            vertical: false,
            autoplaySpeed: 3000,
            swipeToSlide: true,
            waitForAnimate: true,
            pauseOnHover: false,
        });
        $('.opportunities-slide').slick({
            autoplay: true,
            vertical: true,
            infinite: true,
