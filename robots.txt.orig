        .pause(2000)
        .delete()
        .type("We are a student-led non-profit organisation.")
        .pause(2000)
        .go();


    gsap.registerPlugin(ScrollTrigger);
    gsap.to(".rotated-box", {
        scrollTrigger: {
            trigger: ".rotated-box",
            scrub: 0.5,
            start: "top center",
            end: "800px top"

        },
        y: -200,

    })
    gsap.to(".box-overlay", {
        scrollTrigger: {
            trigger: ".box-overlay",
            scrub: 0.5,
            start: "top 400px",
            end: "top top"

        },
        y: -100,

    })
    //CODE FOR SLICK

    $(document).ready(function () {
        $('#goals-carousel').slick({
            autoplay: true,
            dots: true,
            infinite: true,
            centerMode: true,
            variableWidth: true,
            adaptiveHeight: true,
