<script type="text/javascript">

    new TypeIt("#landing-typing", {
        speed: 40,
        loop: true,

    }).type("We will be your all-in-one source for all EU related information.")
        .pause(1000)
        .type("Study, travel, volunteering, job and decison-making opportunities...")
        .pause(2000)
        .delete()
        .type("We will be 100% for FREE.")
        .pause(2000)
